<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0
                      http://maven.apache.org/xsd/settings-1.0.0.xsd">

    <servers>
        <server>
            <id>PPRBAC_release</id>
            <username>${server.username}</username>
            <password>${server.password}</password>
        </server>
        <server>
            <id>PPRBAC_snapshot</id>
            <username>${server.username}</username>
            <password>${server.password}</password>
        </server>
        <server>
            <id>PPRB_dev_group</id>
            <username>${server.username}</username>
            <password>${server.password}</password>
        </server>
        <server>
            <id>INTLAB_thirdparty</id>
            <username>${server.username}</username>
            <password>${server.password}</password>
        </server>
        <server>
            <id>distribution.repo2</id>
            <username>${server.distribution.username}</username>
            <password>${server.distribution.password}</password>
        </server>
        <server>
            <id>DOG900_thirdparty</id>
            <username>${server.username}</username>
            <password>${server.password}</password>
        </server>
    </servers>

    <profiles>
        <profile>
            <id>artifactory</id>
            <repositories>
                <repository>
                    <id>PPRB_dev_group</id>
                    <url>http://sbtnexus.ca.sbrf.ru:8081/nexus/content/repositories/PPRB_dev_group</url>
                </repository>
                <repository>
                    <id>INTLAB_thirdparty</id>
                    <url>http://sbtnexus.ca.sbrf.ru:8081/nexus/content/repositories/INTLAB_thirdparty</url>
                </repository>
                <repository>
                    <id>DOG900_thirdparty</id>
                    <url>http://sbtnexus.ca.sbrf.ru:8081/nexus/content/repositories/DOG900_thirdparty</url>
                </repository>
            </repositories>
            <pluginRepositories>
                <pluginRepository>
                    <id>PPRB_dev_group</id>
                    <url>http://sbtnexus.ca.sbrf.ru:8081/nexus/content/repositories/PPRB_dev_group</url>
                </pluginRepository>
            </pluginRepositories>
        </profile>
    </profiles>

    <activeProfiles>
        <activeProfile>artifactory</activeProfile>
    </activeProfiles>
</settings>