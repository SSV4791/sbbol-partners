{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "javaType": "ru.sberbank.pprb.partners.model.PartnerCreate",
  "type": "object",
  "title": "Партнер",
  "properties": {
    "digitalId": {
      "type": "string",
      "title": "Идентификатор личного кабинета клиента",
      "minLength": 1,
      "maxLength": 40
    },
    "legalForm": {
      "$ref": "../common/legal_form.json"
    },
    "orgName": {
      "type": "string",
      "title": "Наименование организации партнера",
      "maxLength": 350
    },
    "firstName": {
      "type": "string",
      "title": "Имя партнера",
      "maxLength": 50
    },
    "secondName": {
      "type": "string",
      "title": "Фамилия партнера",
      "maxLength": 50
    },
    "middleName": {
      "type": "string",
      "title": "Отчество партнера",
      "maxLength": 50
    },
    "inn": {
      "type": "string",
      "title": "ИНН",
      "minLength": 5,
      "maxLength": 12
    },
    "kpp": {
      "type": "string",
      "title": "КПП",
      "minLength": 9,
      "maxLength": 9
    },
    "ogrn": {
      "type": "string",
      "title": "ОГРН",
      "minLength": 13,
      "maxLength": 15
    },
    "okpo": {
      "type": "string",
      "title": "ОКПО",
      "minLength": 8,
      "maxLength": 10
    },
    "phones": {
      "type": "array",
      "title": "Номера телефона контакта",
      "items": {
        "type": "string"
      },
      "minItems": 0,
      "uniqueItems": true
    },
    "emails": {
      "type": "array",
      "title": "Номера телефона контакта",
      "items": {
        "type": "string"
      },
      "minItems": 0,
      "uniqueItems": true
    },
    "comment": {
      "type": "string",
      "title": "Комментарий пользователя",
      "maxLength": 255
    },
    "citizenship": {
      "$ref": "../common/citizenship.json"
    }
  },
  "required": [
    "digitalId",
    "legalForm"
  ]
}
