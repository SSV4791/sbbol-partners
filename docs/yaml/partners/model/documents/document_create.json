{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "javaType": "ru.sberbank.pprb.partners.model.DocumentCreate",
  "type": "object",
  "title": "Документ",
  "properties": {
    "unifiedId": {
      "type": "string",
      "title": "Идентификатор партнера или контакта",
      "minLength": 36,
      "maxLength": 36,
      "example": "8c4e48ff-1d06-47ab-83b1-fb6683cab6da"
    },
    "digitalId": {
      "type": "string",
      "title": "Идентификатор личного кабинета клиента",
      "minLength": 1,
      "maxLength": 40
    },
    "documentTypeId": {
      "type": "string",
      "title": "Идентификатор типа документа",
      "minLength": 36,
      "maxLength": 36,
      "example": "8c4e48ff-1d06-47ab-83b1-fb6683cab6da"
    },
    "series": {
      "type": "string",
      "title": "Серия документа",
      "maxLength": 50
    },
    "number": {
      "type": "string",
      "title": "Номер документа",
      "maxLength": 50
    },
    "dateIssue": {
      "type": "string",
      "format": "date",
      "title": "Дата выдачи документа",
      "example": 20200101
    },
    "divisionIssue": {
      "type": "string",
      "title": "Место выдачи документа",
      "maxLength": 250
    },
    "divisionCode": {
      "type": "string",
      "title": "Код документа",
      "maxLength": 50
    },
    "certifierName": {
      "type": "string",
      "title": "Имя удостоверяющего сотрудника",
      "maxLength": 100
    },
    "positionCertifier": {
      "type": "string",
      "title": "Должность удостоверяющего сотрудника",
      "maxLength": 100
    },
    "certifierType": {
      "$ref": "../common/certifier_type.json"
    }
  },
  "required": [
    "digitalId",
    "unifiedId"
  ]
}

