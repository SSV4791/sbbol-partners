{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "javaType": "ru.sberbank.pprb.partners.model.AccountChange",
  "type": "object",
  "title": "Счёт партнера",
  "properties": {
    "id": {
      "type": "string",
      "title": "Идентификатор счёта",
      "minLength": 36,
      "maxLength": 36,
      "example": "8c4e48ff-1d06-47ab-83b1-fb6683cab6da"
    },
    "partnerId": {
      "type": "string",
      "title": "Идентификатор партнера",
      "minLength": 36,
      "maxLength": 36,
      "example": "8c4e48ff-1d06-47ab-83b1-fb6683cab6da"
    },
    "digitalId": {
      "type": "string",
      "title": "Идентификатор личного кабинета клиента",
      "minLength": 1,
      "maxLength": 40
    },
    "version": {
      "type": "integer",
      "format": "int64",
      "title": "Версия"
    },
    "account": {
      "type": "string",
      "title": "Счёт"
    },
    "bank": {
      "title": "Банк",
      "$ref": "../../model/bank/bank.json"
    },
    "comment": {
      "type": "string",
      "title": "Комментарий счета",
      "minLength": 1,
      "maxLength": 50
    }
  },
  "required": [
    "version",
    "id",
    "digitalId",
    "partnerId"
  ]
}
