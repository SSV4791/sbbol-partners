{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "javaType": "ru.sberbank.pprb.partners.model.ContactFilter",
  "type": "object",
  "title": "Фильтр контактов партнера",
  "properties": {
    "partnerId": {
      "type": "string",
      "title": "Идентификатор партнера",
      "minLength": 36,
      "maxLength": 36,
      "example": "8c4e48ff-1d06-47ab-83b1-fb6683cab6da"
    },
    "ids": {
      "type": "array",
      "title": "Идентификаторы контактов",
      "items": {
        "type": "string",
        "minLength": 36,
        "maxLength": 36,
        "example": "8c4e48ff-1d06-47ab-83b1-fb6683cab6da"
      }
    },
    "digitalId": {
      "type": "string",
      "title": "Идентификатор личного кабинета клиента",
      "minLength": 1,
      "maxLength": 40
    },
    "pagination": {
      "$ref": "../../../common/model/pagination.json",
      "title": "Параметры запроса Контактов"
