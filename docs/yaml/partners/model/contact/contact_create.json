{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "javaType": "ru.sberbank.ufs.partner.model.ContactCreate",
  "type": "object",
  "title": "Контакт",
  "properties": {
    "partnerId": {
      "type": "string",
      "title": "Идентификатор партнера",
      "minLength": 36,
      "maxLength": 36,
      "example": "8c4e48ff-1d06-47ab-83b1-fb6683cab6da"
    },
    "digitalId": {
      "type": "string",
      "title": "Идентификатор личного кабинета клиента",
      "minLength": 1,
      "maxLength": 40
    },
    "orgName": {
      "type": "string",
      "title": "Наименование организации",
      "maxLength": 350
    },
    "firstName": {
      "type": "string",
      "title": "Имя контакта",
      "maxLength": 50
    },
    "secondName": {
      "type": "string",
      "title": "Фамилия контакта",
      "maxLength": 50
    },
    "middleName": {
      "type": "string",
      "title": "Отчество контакта",
      "maxLength": 50
    },
    "position": {
      "type": "string",
      "title": "Должность",
      "maxLength": 100
    },
    "phones": {
      "type": "array",
      "title": "Номера телефона контакта",
      "items": {
        "type": "string"
      },
      "minItems": 0,
      "uniqueItems": true
    },
    "emails": {
      "type": "array",
      "title": "Номера телефона контакта",
      "items": {
        "type": "string"
      },
      "minItems": 0,
      "uniqueItems": true
    },
    "legalForm": {
      "$ref": "../common/legal_form.json"
    }
  },
  "required": [
    "partnerId",
    "digitalId",
    "legalForm"
  ]
}
