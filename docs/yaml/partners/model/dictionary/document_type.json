{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "javaType": "ru.sberbank.pprb.partners.model.DocumentType",
  "type": "object",
  "title": "Вид документа",
  "properties": {
    "id": {
      "type": "string",
      "title": "Идентификатор",
      "minLength": 36,
      "maxLength": 36,
      "example": "8c4e48ff-1d06-47ab-83b1-fb6683cab6da"
    },
    "documentType": {
      "type": "string",
      "title": "Тип удостоверяющего документа (физ. лицо)",
      "maxLength": 50
    },
    "description": {
      "type": "string",
      "title": "Описание Типа удостоверяющего документа",
      "maxLength": 100
    },
    "deleted": {
      "type": "boolean",
      "title": "Признак 'удалён' если в 'true'",
      "default": false
    },
    "legalForms": {
      "type": "array",
      "title": "Тип субъекта документа",
      "items": {
        "$ref": "../common/legal_form.json"
      }
    }
  },
  "required": [
    "id",
    "documentType",
    "description"
  ]
}
