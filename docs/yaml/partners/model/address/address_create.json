{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "javaType": "ru.sberbank.pprb.partners.model.AddressCreate",
  "type": "object",
  "title": "Адрес партнера",
  "properties": {
    "unifiedId": {
      "type": "string",
      "title": "Идентификатор партнера/контакта",
      "minLength": 36,
      "maxLength": 36,
      "example": "8c4e48ff-1d06-47ab-83b1-fb6683cab6da"
    },
    "digitalId": {
      "type": "string",
      "title": "Идентификатор личного кабинета клиента",
      "minLength": 1,
      "maxLength": 40
    },
    "type": {
      "$ref": "../common/address_type.json"
    },
    "zipCode": {
      "type": "string",
      "title": "Индекс",
      "maxLength": 6
    },
    "region": {
      "type": "string",
      "title": "Регион",
      "maxLength": 50
    },
    "regionCode": {
      "type": "string",
      "title": "Код региона",
      "maxLength": 10
    },
    "city": {
      "type": "string",
      "title": "Город",
      "maxLength": 300
    },
    "location": {
      "type": "string",
      "title": "Населённый пункт",
      "maxLength": 300
    },
    "street": {
      "type": "string",
      "title": "Улица",
      "maxLength": 300
    },
    "building": {
      "type": "string",
      "title": "Дом",
      "maxLength": 100
    },
    "buildingBlock": {
      "type": "string",
      "title": "Строение",
      "maxLength": 20
    },
    "flat": {
      "type": "string",
      "title": "Квартира",
      "maxLength": 10
    }
  },
  "required": [
    "unifiedId",
    "digitalId",
    "type"
  ]
}
