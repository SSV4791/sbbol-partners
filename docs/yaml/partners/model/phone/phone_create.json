{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "javaType": "ru.sberbank.pprb.partners.model.PhoneCreate",
  "type": "object",
  "title": "Телефон",
  "properties": {
    "unifiedId": {
      "type": "string",
      "title": "Идентификатор партнера или контакта",
      "minLength": 36,
      "maxLength": 36,
      "example": "8c4e48ff-1d06-47ab-83b1-fb6683cab6da"
    },
    "digitalId": {
      "type": "string",
      "title": "Идентификатор личного кабинета клиента",
      "minLength": 1,
      "maxLength": 40
    },
    "phone": {
      "type": "string",
      "title": "Номер телефона",
      "maxLength": 12,
      "pattern": "^[+]?[0-9]{1,11}$",
      "example": "+89121112233"
    }
  },
  "required": [
    "digitalId",
    "phone",
    "unifiedId"
  ]
}
