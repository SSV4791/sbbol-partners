{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "javaType": "ru.sberbank.pprb.partners.model.Email",
  "type": "object",
  "title": "Электронный адрес",
  "properties": {
    "id": {
      "type": "string",
      "title": "Идентификатор электронного адреса",
      "minLength": 36,
      "maxLength": 36,
      "example": "8c4e48ff-1d06-47ab-83b1-fb6683cab6da"
    },
    "digitalId": {
      "type": "string",
      "title": "Идентификатор личного кабинета клиента",
      "minLength": 1,
      "maxLength": 40
    },
    "unifiedId": {
      "type": "string",
      "title": "Идентификатор партнера или контакта",
      "minLength": 36,
      "maxLength": 36,
      "example": "8c4e48ff-1d06-47ab-83b1-fb6683cab6da"
    },
    "email": {
      "type": "string",
      "title": "Электронный адрес",
      "maxLength": 320,
      "pattern": "^(.){1,64}@(.){1,255}$",
      "example": "test@sberbank.ru"
    },
    "version": {
      "type": "integer",
      "format": "int64",
      "title": "Версия"
    }
  },
  "required": [
    "version",
    "digitalId",
    "id",
    "unifiedId"
  ]
}
