{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "javaType": "ru.sberbank.pprb.partners.model.EmailAddressFilter",
  "type": "object",
  "title": "Фильтр для запроса электронных адресов партнера/контакта",
  "properties": {
    "digitalId": {
      "type": "string",
      "title": "Идентификатор личного кабинета клиента",
      "minLength": 1,
      "maxLength": 40
    },
    "unifiedIds": {
      "type": "array",
      "items": {
        "type": "string",
        "minLength": 36,
        "maxLength": 36,
        "example": "8c4e48ff-1d06-47ab-83b1-fb6683cab6da"
      },
      "title": "Идентификаторы партнеров/контактов"
    },
    "pagination": {
      "$ref": "../../../common/model/pagination.json",
      "title": "Параметры запроса электронных адресов"
    }
  },
  "required": [
    "unifiedIds",
    "digitalId",
    "pagination"
  ]
}
