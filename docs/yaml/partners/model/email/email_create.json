{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "javaType": "ru.sberbank.pprb.partners.model.EmailCreate",
  "type": "object",
  "title": "Электронный адрес",
  "properties": {
    "unifiedId": {
      "type": "string",
      "title": "Идентификатор партнера или контакта",
      "minLength": 36,
      "maxLength": 36,
      "example": "8c4e48ff-1d06-47ab-83b1-fb6683cab6da"
    },
    "digitalId": {
      "type": "string",
      "title": "Идентификатор личного кабинета клиента",
      "minLength": 1,
      "maxLength": 40
    },
    "email": {
      "type": "string",
      "title": "Электронный адрес",
      "maxLength": 100
    }
  },
  "required": [
    "digitalId",
    "unifiedId"
  ]
}
