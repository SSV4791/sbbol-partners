{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "javaType": "ru.sberbank.pprb.partners.model.AccountSign",
  "type": "object",
  "title": "Подпись счетов",
  "properties": {
    "digitalId": {
      "type": "string",
      "title": "Идентификатор личного кабинета клиента",
      "minLength": 1,
      "maxLength": 40
    },
    "accountId": {
      "type": "string",
      "title": "Идентификатор счёта или контрагента",
      "minLength": 36,
      "maxLength": 36,
      "example": "8c4e48ff-1d06-47ab-83b1-fb6683cab6da"
    },
    "state": {
      "type": "string",
      "title": "Признак 'Подписан'",
      "enum": [
        "SIGNED",
        "NOT_SIGNED"
      ]
    }
  },
  "required": [
    "digitalId",
    "accountId"
  ]
}
