{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "javaType": "ru.sberbank.pprb.partners.model.AccountSignFilter",
  "type": "object",
  "title": "Запрос подписи по фильтру",
  "properties": {
    "digitalId": {
      "type": "string",
      "title": "Идентификатор личного кабинета клиента",
      "minLength": 1,
      "maxLength": 40
    },
    "partnerId": {
      "type": "string",
      "title": "Идентификатор партнера",
      "minLength": 36,
      "maxLength": 36,
      "example": "8c4e48ff-1d06-47ab-83b1-fb6683cab6da"
    },
    "accountsId": {
      "type": "array",
      "title": "Идентификатор счетов",
      "items": {
        "type": "string",
        "minLength": 36,
        "maxLength": 36,
        "example": "8c4e48ff-1d06-47ab-83b1-fb6683cab6da"
      }
    },
    "pagination": {
      "$ref": "../../../common/model/pagination.json",
      "title": "Параметры запроса Партнеров"
    }
  },
  "required": [
    "pagination",
    "partnerId",
    "digitalId"
  ]
}

