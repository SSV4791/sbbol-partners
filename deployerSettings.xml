<settings>
    <deployer>
        <tagForArtifacts>
            <artifact>
                <groupId>ru.sberbank.pprb.sbbol.partners</groupId>
                <artifactId>partners</artifactId>
            </artifact>
        </tagForArtifacts>
        <ignoreTestDependencies>true</ignoreTestDependencies>
        <ignoreProvidedDependencies>true</ignoreProvidedDependencies>
        <ignoreTestSources>true</ignoreTestSources>
        <additionalUnmanagedFiles>
            <additionalFile>./model/src/main/resources/model/model/changelog.xml</additionalFile>
            <additionalFile>./model/src/main/resources/model/model/pdm.xml</additionalFile>
        </additionalUnmanagedFiles>
        <additionalFiles>
            <additionalFile>./model/src/main/resources/model/model.xml</additionalFile>
        </additionalFiles>
    </deployer>
    <maven>
        <options>-P meta-export</options>
    </maven>
    <version>
        <inheritParentVersion>true</inheritParentVersion>
        <inheritProjectVersion>true</inheritProjectVersion>
        <format>PPRB_1_1_HF1</format>
        <forceUpdate>true</forceUpdate>
    </version>
    <git>
        <tagPrefix>SBP-</tagPrefix>
    </git>
