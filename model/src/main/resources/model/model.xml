<model model-name="partners" version="DEV-SNAPSHOT">

    <class name="Renter" label="Арендатор">
        <property name="uuid" type="String" mandatory="true" length="36" label="Используется как уникальный идентификатор записи"/>
        <property name="renterType" type="RenterType" label="Тип" mandatory="true"/>
        <property name="legalName" type="String" label="Наименование ЮЛ/ИП" length="350"/>
        <property name="inn" type="String" label="ИНН" length="12"/>
        <property name="kpp" type="String" label="КПП" length="9"/>
        <property name="ogrn" type="String" label="ОГРН" length="15"/>
        <property name="okpo" type="String" label="ОКПО" length="30"/>
        <property name="lastName" type="String" label="Фамилия" length="50"/>
        <property name="firstName" type="String" label="Имя" length="50"/>
        <property name="middleName" type="String" label="Отчество" length="50"/>
        <property name="dulType" type="DulType" label="Тип удостоверяющего документа (физ. лицо)" length="2"/>
        <property name="dulSerie" type="String" label="Серия ДУ" length="50"/>
        <property name="dulNumber" type="String" label="Номер ДУЛ" length="50"/>
        <property name="dulDivisionIssue" type="String" label="Кем выдан ДУЛ" length="250"/>
        <property name="dulDateIssue" type="LocalDate" label="Дата выдачи ДУЛ" length="10"/>
        <property name="dulDivisionCode" type="String" label="Код подразделения из ДУЛ" length="50"/>

        <property name="account" type="String" label="Счет" length="20"/>
        <property name="bankBic" type="String" label="БИК банка" length="9"/>
        <property name="bankName" type="String" label="Наименование банка" length="160"/>
        <property name="bankAccount" type="String" label="К/с банка" length="20"/>
        <property name="phoneNumbers" type="String" label="Телефоны" length="100"/>
        <property name="emails" type="String" label="E-mail" length="320"/>
        <reference name="legalAddress" type="Address" label="Адрес регистрации"/>
        <reference name="physicalAddress" type="Address" label="Фактический адрес"/>
    </class>

    <class name="Address" label="Адрес">
        <property name="zipCode" type="String" label="Индекс" length="6" mandatory="true"/>
        <property name="regionCode" type="String" label="Код региона" length="10"/>
        <property name="region" type="String" label="Район" length="50" mandatory="true"/>
        <property name="city" type="String" label="Город" length="300" mandatory="true"/>
        <property name="locality" type="String" label="Населенный пункт" length="300" mandatory="true"/>
        <property name="street" type="String" label="Улица" length="300" mandatory="true"/>
        <property name="building" type="String" label="Дом" length="100" mandatory="true"/>
        <property name="buildingBlock" type="String" label="Корпус" length="20" mandatory="true"/>
        <property name="flat" type="String" label="Квартира" length="20" mandatory="true"/>
    </class>

    <enum name="RenterType">
        <value name="LEGAL_ENTITY">
            <extension name="desc" value="Юр. лицо"/>
        </value>
        <value name="ENTREPRENEUR">
            <extension name="desc" value="ИП"/>
        </value>
        <value name="PHYSICAL_PERSON">
            <extension name="desc" value="Физ. лицо"/>
        </value>
    </enum>

    <enum name="DulType">
        <value name="PASSPORTOFRUSSIA">
            <extension name="desc" value="Паспорт гражданина РФ"/>
        </value>
        <value name="SEAMANPASSPORT">
            <extension name="desc" value="Паспорт моряка (удостоверение личности моряка)"/>
        </value>
        <value name="SERVICEMANIDENTITYCARDOFRUSSIA">
            <extension name="desc" value="Удостоверение личности военнослужащего"/>
        </value>
        <value name="FOREIGNPASSPORT">
            <extension name="desc" value="Паспорт иностранного гражданина"/>
        </value>
        <value name="SERVICEPASSPORTOFRUSSIA">
            <extension name="desc" value="Служебный паспорт гражданина РФ"/>
        </value>
        <value name="RFCITIZENDIPLOMATICPASSPORT">
            <extension name="desc" value="Дипломатический паспорт"/>
        </value>
        <value name="PASSPORTOFRUSSIAWITHCHIP">
            <extension name="desc" value="Паспорт гражданина РФ, содержащий электронный носитель информации"/>
        </value>
    </enum>

</model>