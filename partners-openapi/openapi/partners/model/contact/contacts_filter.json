{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "title": "Фильтр контактов партнера",
  "properties": {
    "partnerId": {
      "type": "string",
      "title": "Идентификатор партнера",
      "minLength": 36,
      "maxLength": 36,
      "x-not-empty": true
    },
    "ids": {
      "type": "array",
      "x-extra-field-annotation": "@UuidFormatArrayValidation",
      "title": "Идентификаторы контактов",
      "maximum": 30,
      "items": {
        "type": "string",
        "minLength": 36,
        "maxLength": 36,
        "x-not-empty": true
      }
    },
    "digitalId": {
      "type": "string",
      "title": "Идентификатор личного кабинета",
      "minLength": 1,
      "maxLength": 40,
      "x-not-empty": true
    },
    "pagination": {
      "$ref": "../common/pagination.json",
      "title": "Параметры запроса Контактов"
    }
  },
  "required": [
    "pagination"
  ]
}

