{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "title": "Контакт",
  "properties": {
    "id": {
      "type": "string",
      "title": "Идентификатор",
      "minLength": 36,
      "maxLength": 36,
      "x-not-empty": true
    },
    "digitalId": {
      "type": "string",
      "title": "Идентификатор личного кабинета",
      "minLength": 1,
      "maxLength": 40,
      "x-not-empty": true
    },
    "partnerId": {
      "type": "string",
      "title": "Идентификатор партнера",
      "minLength": 36,
      "maxLength": 36,
      "x-not-empty": true
    },
    "orgName": {
      "type": "string",
      "title": "Наименование организации",
      "maxLength": 350,
      "x-not-empty": true
    },
    "version": {
      "type": "integer",
      "format": "int64",
      "title": "Версия"
    },
    "firstName": {
      "type": "string",
      "title": "Имя контакта",
      "maxLength": 50,
      "x-not-empty": true
    },
    "secondName": {
      "type": "string",
      "title": "Фамилия контакта",
      "maxLength": 50,
      "x-not-empty": true
    },
    "middleName": {
      "type": "string",
      "title": "Отчество контакта",
      "maxLength": 50,
      "x-not-empty": true
    },
    "position": {
      "type": "string",
      "title": "Должность",
      "maxLength": 100,
      "x-not-empty": true
    },
    "phones": {
      "type": "array",
      "title": "Номера телефона контакта",
      "items": {
        "$ref": "../phone/phone.json"
      },
      "uniqueItems": true
    },
    "emails": {
      "type": "array",
      "title": "Адреса электронной почты контакта",
      "items": {
        "$ref": "../email/email.json"
      },
      "uniqueItems": true
    },
    "legalForm": {
      "$ref": "../common/legal_form.json"
    }
  },
  "required": [
    "id",
    "digitalId",
    "partnerId",
    "version"
  ]
}
