{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "javaType": "ru.sberbank.pprb.partners.model.Contact",
  "type": "object",
  "title": "Контакт",
  "properties": {
    "id": {
      "type": "string",
      "title": "Идентификатор"
    },
    "digitalId": {
      "type": "string",
      "title": "Идентификатор личного кабинета"
    },
    "orgName": {
      "type": "string",
      "title": "Наименование организации"
    },
    "version": {
      "type": "integer",
      "format": "int64",
      "title": "Версия"
    },
    "partnerId": {
      "type": "string",
      "title": "Идентификатор партнера"
    },
    "firstName": {
      "type": "string",
      "title": "Имя контакта"
    },
    "secondName": {
      "type": "string",
      "title": "Фамилия контакта"
    },
    "middleName": {
      "type": "string",
      "title": "Отчество контакта"
    },
    "position": {
      "type": "string",
      "title": "Должность"
    },
    "phones": {
      "type": "array",
      "title": "Номера телефона контакта",
      "items": {
        "$ref": "../phone/phone.json"
      }
    },
    "emails": {
      "type": "array",
      "title": "Адреса электронной почты контакта",
      "items": {
        "$ref": "../email/email.json"
      }
    },
    "legalForm": {
      "$ref": "../common/legal_form.json"
    }
  }
}
