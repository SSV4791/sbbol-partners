{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "javaType": "ru.sberbank.ufs.partner.model.ContactCreate",
  "type": "object",
  "title": "Контакт",
  "properties": {
    "partnerId": {
      "type": "string",
      "title": "Идентификатор партнера"
    },
    "digitalId": {
      "type": "string",
      "title": "Идентификатор личного кабинета клиента"
    },
    "orgName": {
      "type": "string",
      "title": "Наименование организации"
    },
    "firstName": {
      "type": "string",
      "title": "Имя контакта"
    },
    "secondName": {
      "type": "string",
      "title": "Фамилия контакта"
    },
    "middleName": {
      "type": "string",
      "title": "Отчество контакта"
    },
    "position": {
      "type": "string",
      "title": "Должность"
    },
    "phones": {
      "type": "array",
      "title": "Номера телефона контакта",
      "items": {
        "$ref": "../phone/phone_create_light.json"
      }
    },
    "emails": {
      "type": "array",
      "title": "Адреса электронной почты контакта",
      "items": {
        "$ref": "../email/email_create_light.json"
      }
    },
    "legalForm": {
      "$ref": "../common/legal_form.json"
    }
  }
}
