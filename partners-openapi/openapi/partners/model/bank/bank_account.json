{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "title": "Корреспондентский счёт банка",
  "properties": {
    "id": {
      "type": "string",
      "title": "Идентификатор",
      "minLength": 36,
      "maxLength": 36,
      "x-not-empty": true
    },
    "bankId": {
      "type": "string",
      "title": "Идентификатор банка",
      "minLength": 36,
      "maxLength": 36,
      "x-not-empty": true
    },
    "version": {
      "type": "integer",
      "format": "int64",
      "title": "Версия"
    },
    "bankAccount": {
      "type": "string",
      "title": "Корреспондентский счёт",
      "pattern": "^(\\d{20})*$",
      "x-pattern-message": "{validation.account.account.pattern}",
      "x-not-empty": true
    }
  }
}
