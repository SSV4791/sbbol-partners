{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "javaType": "ru.sberbank.pprb.partners.model.Bank",
  "type": "object",
  "title": "Банк",
  "properties": {
    "id": {
      "type": "string",
      "title": "Идентификатор"
    },
    "accountId": {
      "type": "string",
      "title": "Идентификатор счёта партнера"
    },
    "version": {
      "type": "integer",
      "format": "int64",
      "title": "Версия"
    },
    "bic": {
      "type": "string",
      "title": "БИК"
    },
    "name": {
      "type": "string",
      "title": "Наименование"
    },
    "mediary": {
      "type": "boolean",
      "title": "Является транзитным банком",
      "default": false
    },
    "bankAccount": {
      "$ref": "bank_account.json",
      "title": "Корреспондентский счёт"
    }
  }
}
