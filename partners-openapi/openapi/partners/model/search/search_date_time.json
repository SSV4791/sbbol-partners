{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "title": "Поиск по дате",
  "properties": {
    "date": {
      "type": "string",
      "format": "date-time",
      "example": "2023-08-15T15:43:00.445053+03:00"
    },
    "condition": {
      "type": "string",
      "enum": [
        "GREATER",
        "LESS"
      ],
      "default": "GREATER"
    }
  }
}
