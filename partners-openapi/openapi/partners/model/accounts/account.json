{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "title": "Счёт партнера",
  "properties": {
    "id": {
      "type": "string",
      "format": "uuid",
      "title": "Идентификатор счёта"
    },
    "partnerId": {
      "type": "string",
      "format": "uuid",
      "title": "Идентификатор партнера"
    },
    "digitalId": {
      "type": "string",
      "title": "Идентификатор личного кабинета клиента"
    },
    "changeDate": {
      "type": "string",
      "format": "date-time",
      "example": "2023-08-15T15:43:00.445053+03:00"
    },
    "version": {
      "type": "integer",
      "format": "int64",
      "title": "Версия"
    },
    "budget": {
      "type": "boolean",
      "title": "Признак бюджетности",
      "default": false
    },
    "account": {
      "type": "string",
      "title": "Счёт"
    },
    "priorityAccount": {
      "type": "boolean",
      "title": "Избранный счёт",
      "default": false
    },
    "bank": {
      "title": "Банк",
      "$ref": "../../model/bank/bank.json"
    },
    "state": {
      "$ref": "../common/sign_type.json"
    },
    "comment": {
      "type": "string",
      "title": "Комментарий счета"
    }
  }
}

