{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "javaType": "ru.sberbank.pprb.partners.model.Account",
  "type": "object",
  "title": "Счёт партнера",
  "properties": {
    "id": {
      "type": "string",
      "title": "Идентификатор счёта"
    },
    "partnerId": {
      "type": "string",
      "title": "Идентификатор партнера"
    },
    "digitalId": {
      "type": "string",
      "title": "Идентификатор личного кабинета клиента"
    },
    "version": {
      "type": "integer",
      "format": "int64",
      "title": "Версия"
    },
    "budget": {
      "type": "boolean",
      "title": "Признак бюджетности",
      "default": false
    },
    "account": {
      "type": "string",
      "title": "Счёт"
    },
    "priority_account": {
      "type": "boolean",
      "title": "Избранный счёт",
      "default": false
    },
    "bank": {
      "title": "Банк",
      "$ref": "../../model/bank/bank.json"
    },
    "state": {
      "type": "string",
      "title": "Признак 'Подписан'",
      "enum": [
        "SIGNED",
        "NOT_SIGNED"
      ]
    },
    "comment": {
      "type": "string",
      "title": "Комментарий счета"
    }
  }
}

