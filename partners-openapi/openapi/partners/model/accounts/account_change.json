{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "title": "Счёт партнера",
  "x-extra-class-annotation": "@AccountAttributeAccountChangeDtoValidation @BankAccountAttributeAccountChangeDtoValidation",
  "properties": {
    "id": {
      "type": "string",
      "title": "Идентификатор счёта",
      "minLength": 36,
      "maxLength": 36,
      "x-not-empty": true
    },
    "partnerId": {
      "type": "string",
      "title": "Идентификатор партнера",
      "minLength": 36,
      "maxLength": 36,
      "x-not-empty": true
    },
    "digitalId": {
      "type": "string",
      "title": "Идентификатор личного кабинета клиента",
      "minLength": 1,
      "maxLength": 40,
      "x-not-empty": true
    },
    "version": {
      "type": "integer",
      "format": "int64",
      "title": "Версия"
    },
    "account": {
      "type": "string",
      "title": "Счёт",
      "maxLength": 20
    },
    "bank": {
      "title": "Банк",
      "$ref": "../../model/bank/bank.json"
    },
    "comment": {
      "type": "string",
      "title": "Комментарий счета",
      "maxLength": 50
    }
  },
  "required": [
    "id",
    "partnerId",
    "digitalId",
    "bank",
    "version"
  ]
}
