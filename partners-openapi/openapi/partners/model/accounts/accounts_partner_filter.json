{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "javaType": "ru.sberbank.pprb.sbbol.partners.model.AccountsPartnerFilter",
  "type": "object",
  "title": "Счетa Партнера",
  "properties": {
    "digitalId": {
      "type": "string",
      "title": "Идентификатор личного кабинета клиента"
    },
    "partnersIds": {
      "type": "array",
      "title": "Идентификаторы партнеров",
      "items": {
        "$ref": "accounts_partner_pagination.json"
      }
    },
    "signStatus": {
      "type": "string",
      "title": "Статус подписания счетов",
      "enum": [
        "SIGNED",
        "NOT_SIGNED"
      ]
    },
    "accountType": {
      "title": "Фильтр для отображения счетов бюджетных организаций",
      "type": "boolean",
      "default": false
    },
    "pagination": {
      "$ref": "../../../common/model/pagination.json",
      "title": "Параметры запроса Счетов"
    },
    "errors": {
      "type": "array",
      "title": "Ошибки",
      "items": {
        "$ref": "../common/error.json"
      }
    }
  }
}
