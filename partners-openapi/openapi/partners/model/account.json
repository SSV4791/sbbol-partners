{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "javaType": "ru.sberbank.pprb.sbbol.partners.model.Account",
  "type": "object",
  "title": "Счёт партнера",
  "properties": {
    "uuid": {
      "type": "string",
      "title": "Идентификатор счёта"
    },
    "version": {
      "type": "integer",
      "title": "Версия"
    },
    "name": {
      "type": "string",
      "title": "Наименование счёта"
    },
    "account": {
      "type": "string",
      "title": "Счёт"
    },
    "bank": {
      "type": "object",
      "title": "Банк",
      "$ref": "bank.json"
    },
    "state": {
      "type": "string",
      "enum": [
        "SIGNED",
        "NOT_SIGNED"
      ],
      "title": "Признак 'Подписан'"
    }
  },
  "required": [
    "uuid"
  ]
}
