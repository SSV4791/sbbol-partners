{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "title": "Электронный адрес",
  "properties": {
    "id": {
      "type": "string",
      "title": "Идентификатор электронного адреса",
      "minLength": 36,
      "maxLength": 36,
      "x-not-empty": true
    },
    "digitalId": {
      "type": "string",
      "title": "Идентификатор личного кабинета клиента",
      "minLength": 1,
      "maxLength": 40,
      "x-not-empty": true
    },
    "unifiedId": {
      "type": "string",
      "title": "Идентификатор партнера или контакта",
      "minLength": 36,
      "maxLength": 36,
      "x-not-empty": true
    },
    "email": {
      "type": "string",
      "format": "email",
      "title": "Электронный адрес"
    },
    "version": {
      "type": "integer",
      "format": "int64",
      "title": "Версия"
    }
  },
  "required": [
    "id",
    "digitalId",
    "unifiedId",
    "version"
  ]
}
