{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "title": "Изменение электронного адреса в рамках изменения партнера и его дочерних сущностей",
  "properties": {
    "id": {
      "type": "string",
      "format": "uuid",
      "title": "Идентификатор электронного адреса"
    },
    "version": {
      "type": "integer",
      "format": "int64",
      "title": "Версия"
    },
    "email": {
      "type": "string",
      "format": "email",
      "title": "Электронный адрес"
    }
  },
  "required": [
    "email"
  ]
}
