{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "javaType": "ru.sberbank.pprb.partners.model.Address",
  "type": "object",
  "title": "Адрес партнера",
  "properties": {
    "id": {
      "type": "string",
      "title": "Идентификатор адреса"
    },
    "version": {
      "type": "integer",
      "format": "int64",
      "title": "Версия"
    },
    "type": {
      "$ref": "../common/address_type.json"
    },
    "digitalId": {
      "type": "string",
      "title": "Идентификатор личного кабинета клиента"
    },
    "unifiedId": {
      "type": "string",
      "title": "Идентификатор партнера/контакта"
    },
    "zipCode": {
      "type": "string",
      "title": "Индекс"
    },
    "region": {
      "type": "string",
      "title": "Район"
    },
    "regionCode": {
      "type": "string",
      "title": "Код района"
    },
    "city": {
      "type": "string",
      "title": "Город"
    },
    "location": {
      "type": "string",
      "title": "Населённый пункт"
    },
    "street": {
      "type": "string",
      "title": "Улица"
    },
    "building": {
      "type": "string",
      "title": "Дом"
    },
    "buildingBlock": {
      "type": "string",
      "title": "Строение"
    },
    "flat": {
      "type": "string",
      "title": "Квартира"
    }
  }
}
