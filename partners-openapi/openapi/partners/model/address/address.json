{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "title": "Адрес партнера",
  "properties": {
    "id": {
      "type": "string",
      "title": "Идентификатор адреса",
      "minLength": 36,
      "maxLength": 36,
      "x-not-empty": true
    },
    "version": {
      "type": "integer",
      "format": "int64",
      "title": "Версия"
    },
    "type": {
      "$ref": "../common/address_type.json"
    },
    "digitalId": {
      "type": "string",
      "title": "Идентификатор личного кабинета клиента",
      "minLength": 1,
      "maxLength": 40,
      "x-not-empty": true
    },
    "unifiedId": {
      "type": "string",
      "title": "Идентификатор партнера/контакта",
      "minLength": 36,
      "maxLength": 36,
      "x-not-empty": true
    },
    "zipCode": {
      "type": "string",
      "title": "Индекс",
      "maxLength": 6
    },
    "region": {
      "type": "string",
      "title": "Район",
      "maxLength": 50
    },
    "regionCode": {
      "type": "string",
      "title": "Код района",
      "maxLength": 10
    },
    "area": {
      "type": "string",
      "title": "Район",
      "maxLength": 300
    },
    "city": {
      "type": "string",
      "title": "Город",
      "maxLength": 300
    },
    "location": {
      "type": "string",
      "title": "Населённый пункт",
      "maxLength": 300
    },
    "street": {
      "type": "string",
      "title": "Улица",
      "maxLength": 300
    },
    "building": {
      "type": "string",
      "title": "Дом",
      "maxLength": 100
    },
    "buildingBlock": {
      "type": "string",
      "title": "Строение",
      "maxLength": 300
    },
    "flat": {
      "type": "string",
      "title": "Квартира",
      "maxLength": 300
    }
  },
  "required": [
    "id",
    "digitalId",
    "unifiedId",
    "version",
    "type"
  ]
}
