{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "title": "Адрес партнера",
  "properties": {
    "type": {
      "$ref": "../common/address_type.json"
    },
    "zipCode": {
      "type": "string",
      "title": "Индекс",
      "maxLength": 6
    },
    "region": {
      "type": "string",
      "title": "Район",
      "maxLength": 50
    },
    "regionCode": {
      "type": "string",
      "title": "Код района",
      "maxLength": 10
    },
    "area": {
      "type": "string",
      "title": "Район",
      "maxLength": 300
    },
    "city": {
      "type": "string",
      "title": "Город",
      "maxLength": 300
    },
    "location": {
      "type": "string",
      "title": "Населённый пункт",
      "maxLength": 300
    },
    "street": {
      "type": "string",
      "title": "Улица",
      "maxLength": 300
    },
    "building": {
      "type": "string",
      "title": "Дом",
      "maxLength": 100
    },
    "buildingBlock": {
      "type": "string",
      "title": "Строение",
      "maxLength": 20
    },
    "flat": {
      "type": "string",
      "title": "Квартира",
      "maxLength": 300
    }
  },
  "required": [
    "type"
  ]
}
