{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "title": "Адрес партнера",
  "properties": {
    "type": {
      "$ref": "../common/address_type.json"
    },
    "zipCode": {
      "type": "string",
      "title": "Индекс",
      "maxLength": 6
    },
    "country_iso_code": {
      "type": "string",
      "title": "ISO-код страны",
      "minLength": 2,
      "maxLength": 3
    },
    "country_code": {
      "type": "string",
      "title": "Код страны",
      "minLength": 3,
      "maxLength": 3
    },
    "country": {
      "type": "string",
      "title": "Наименование страны",
      "maxLength": 80
    },
    "administration_unit_code": {
      "type": "string",
      "title": "Код административной единицы",
      "maxLength": 10
    },
    "administration_unit": {
      "type": "string",
      "title": "Наименование административной единицы",
      "maxLength": 70
    },
    "full_address": {
      "type": "string",
      "title": "Полная строка адреса",
      "maxLength": 1024
    },
    "region": {
      "type": "string",
      "title": "Район",
      "maxLength": 50
    },
    "regionCode": {
      "type": "string",
      "title": "Код района",
      "maxLength": 10
    },
    "area": {
      "type": "string",
      "title": "Район",
      "maxLength": 300
    },
    "city": {
      "type": "string",
      "title": "Город",
      "maxLength": 300
    },
    "location": {
      "type": "string",
      "title": "Населённый пункт",
      "maxLength": 300
    },
    "street": {
      "type": "string",
      "title": "Улица",
      "maxLength": 300
    },
    "building": {
      "type": "string",
      "title": "Дом",
      "maxLength": 100
    },
    "buildingBlock": {
      "type": "string",
      "title": "Строение",
      "maxLength": 20
    },
    "flat": {
      "type": "string",
      "title": "Квартира",
      "maxLength": 300
    }
  },
  "required": [
    "type"
  ]
}
