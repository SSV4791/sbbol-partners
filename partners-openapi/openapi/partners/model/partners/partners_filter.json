{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "javaType": "ru.sberbank.pprb.sbbol.partners.model.PartnersFilter",
  "type": "object",
  "title": "Запрос Партнеров по фильтру",
  "properties": {
    "digitalId": {
      "type": "string",
      "title": "Идентификатор личного кабинета клиента"
    },
    "search": {
      "title": "Поиск партнера по имени, названию компании, инн",
      "type": "string",
      "$ref": "../search/search_partners.json"
    },
    "partnersType": {
      "title": "Фильтр по видам партнеров и правовой формы ЮЛ",
      "type": "string",
      "enum": [
        "LEGAL_ENTITY",
        "ENTREPRENEUR",
        "PHYSICAL_PERSON",
        "GKU",
        "BUDGET"
      ]
    },
    "accountSignType": {
      "type": "string",
      "title": "Фильтр по видам счетов партнеров",
      "enum": [
        "SIGNED",
        "NOT_SIGNED"
      ]
    },
    "pagination": {
      "$ref": "../../../common/model/pagination.json",
      "title": "Параметры запроса Партнеров"
    }
  }
}
