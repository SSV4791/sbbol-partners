{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "title": "Удаление партнера",
  "properties": {
    "digitalId": {
      "type": "string",
      "title": "Идентификатор личного кабинета клиента",
      "minLength": 1,
      "maxLength": 40,
      "x-not-empty": true
    },
    "partnerIds": {
      "type": "array",
      "title": "Идентификаторы Партнеров",
      "items": {
        "type": "string",
        "minLength": 36,
        "maxLength": 36
      },
      "uniqueItems": true
    },
    "fraudMetaData": {
      "type": "object",
      "$ref": "../fraud/fraud_meta_data.json",
      "title": "Данные для отправки во ФМ ЮЛ "
    }
  },
  "required": [
    "digitalId",
    "partnerIds",
    "fraudMetaData"
  ]
}
