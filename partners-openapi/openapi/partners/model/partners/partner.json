{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "javaType": "ru.sberbank.pprb.partners.model.Partner",
  "type": "object",
  "title": "Партнер",
  "properties": {
    "id": {
      "type": "string",
      "title": "Уникальный идентификатор партнера"
    },
    "digitalId": {
      "type": "string",
      "title": "Идентификатор личного кабинета клиента"
    },
    "version": {
      "type": "integer",
      "format": "int64",
      "title": "Версия"
    },
    "legalForm": {
      "$ref": "../common/legal_form.json"
    },
    "orgName": {
      "type": "string",
      "title": "Наименование организации партнера"
    },
    "firstName": {
      "type": "string",
      "title": "Имя партнера"
    },
    "secondName": {
      "type": "string",
      "title": "Фамилия партнера"
    },
    "middleName": {
      "type": "string",
      "title": "Отчество партнера"
    },
    "inn": {
      "type": "string",
      "title": "ИНН"
    },
    "kpp": {
      "type": "string",
      "title": "КПП"
    },
    "ogrn": {
      "type": "string",
      "title": "ОГРН"
    },
    "okpo": {
      "type": "string",
      "title": "ОКПО"
    },
    "phones": {
      "type": "array",
      "title": "Номера телефонов партнера",
      "items": {
        "$ref": "../phone/phone.json"
      }
    },
    "emails": {
      "type": "array",
      "title": "Адреса электронной почты партнера",
      "items": {
        "$ref": "../email/email.json"
      }
    },
    "comment": {
      "type": "string",
      "title": "Комментарий пользователя"
    },
    "gku": {
      "type": "boolean",
      "title": "Признак ЖКУ",
      "default": false
    },
    "budget": {
      "type": "boolean",
      "title": "Признак бюджетности",
      "default": false
    },
    "citizenship": {
      "$ref": "../common/citizenship.json"
    }
  }
}
