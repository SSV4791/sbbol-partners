{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "javaType": "ru.sberbank.pprb.sbbol.partners.model.Bank",
  "type": "object",
  "title": "Банк",
  "properties": {
    "uuid": {
      "type": "string",
      "title": "Идентификатор"
    },
    "version": {
      "type": "integer",
      "format": "int64",
      "title": "Версия"
    },
    "bic": {
      "type": "string",
      "title": "БИК"
    },
    "name": {
      "type": "string",
      "title": "Наименование"
    },
    "accounts": {
      "type": "array",
      "title": "Корреспондентский счёт",
      "items": {
        "$ref": "bank_account.json"
      }
    }
  },
  "required": [
    "uuid"
  ]
}
