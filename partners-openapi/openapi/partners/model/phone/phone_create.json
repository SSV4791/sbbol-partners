{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "title": "Телефон",
  "properties": {
    "unifiedId": {
      "type": "string",
      "title": "Идентификатор партнера или контакта",
      "minLength": 36,
      "maxLength": 36,
      "x-not-empty": true
    },
    "digitalId": {
      "type": "string",
      "title": "Идентификатор личного кабинета клиента",
      "minLength": 1,
      "maxLength": 40,
      "x-not-empty": true
    },
    "phone": {
      "type": "string",
      "title": "Номер телефона",
      "minLength": 13,
      "maxLength": 13,
      "pattern": "^\\d{13}$"
    }
  }
}
