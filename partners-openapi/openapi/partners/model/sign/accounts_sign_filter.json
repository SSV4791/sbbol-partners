{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "title": "Запрос подписи по фильтру",
  "properties": {
    "digitalId": {
      "type": "string",
      "title": "Идентификатор личного кабинета клиента",
      "minLength": 1,
      "maxLength": 40,
      "x-not-empty": true
    },
    "partnerId": {
      "type": "string",
      "title": "Идентификатор партнера",
      "minLength": 36,
      "maxLength": 36,
      "x-not-empty": true
    },
    "accountsId": {
      "type": "array",
      "title": "Идентификатор счетов",
      "items": {
        "type": "string",
        "minLength": 36,
        "maxLength": 36,
        "x-not-empty": true
      }
    },
    "pagination": {
      "$ref": "../../../common/model/pagination.json",
      "title": "Параметры запроса Партнеров"
    }
  },
  "required": [
    "pagination"
  ]
}

