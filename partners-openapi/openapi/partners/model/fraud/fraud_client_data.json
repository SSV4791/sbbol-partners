{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "title": "Данные пользователя",
  "properties": {
    "digitalId": {
      "type": "string",
      "title": "Идентификатор кабинета",
      "maxLength": 2000,
      "x-not-empty": true
    },
    "epkId": {
      "type": "string",
      "title": "Идентификатор ЕПК ЮЛ",
      "x-not-empty": true
    },
    "login": {
      "type": "string",
      "title": "Логин пользователя",
      "maxLength": 50,
      "x-not-empty": true
    },
    "inn": {
      "type": "string",
      "title": "ИНН клиента",
      "maxLength": 2000,
      "x-not-empty": true
    },
    "orgName": {
      "type": "string",
      "title": "Наименование организации",
      "maxLength": 2000,
      "x-not-empty": true
    },
    "phone": {
      "type": "string",
      "title": "Номер телефона",
      "maxLength": 13,
      "x-not-empty": true
    },
    "email": {
      "type": "string",
      "title": "Электронный адрес",
      "maxLength": 320,
      "x-not-empty": true
    },
    "terBankNumber": {
      "type": "string",
      "title": "Код территориального банка",
      "maxLength": 50,
      "x-not-empty": true
    },
    "gosbNumber": {
      "type": "string",
      "title": "Номер ОСБ",
      "maxLength": 2000,
      "x-not-empty": true
    },
    "vspNumber": {
      "type": "string",
      "title": "Номер ВСП",
      "maxLength": 2000,
      "x-not-empty": true
    }
  },
  "required": [
    "digitalId",
    "epkId",
    "login",
    "inn",
    "orgName",
    "phone",
    "email",
    "terBankNumber",
    "gosbNumber",
    "vspNumber"
  ]
}
