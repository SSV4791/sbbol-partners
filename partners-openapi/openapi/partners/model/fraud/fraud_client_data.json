{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "title": "Данные пользователя",
  "properties": {
    "digitalId": {
      "type": "string",
      "title": "Идентификатор кабинета",
      "maxLength": 2000,
      "x-not-empty": true
    },
    "epkId": {
      "type": "string",
      "title": "Идентификатор ЕПК ЮЛ"
    },
    "login": {
      "type": "string",
      "title": "Логин пользователя",
      "maxLength": 50
    },
    "inn": {
      "type": "string",
      "title": "ИНН клиента",
      "maxLength": 2000
    },
    "orgName": {
      "type": "string",
      "title": "Наименование организации",
      "maxLength": 2000
    },
    "phone": {
      "type": "string",
      "title": "Номер телефона",
      "maxLength": 13
    },
    "email": {
      "type": "string",
      "title": "Электронный адрес",
      "maxLength": 320
    },
    "terBankNumber": {
      "type": "string",
      "title": "Код территориального банка",
      "maxLength": 50
    },
    "gosbNumber": {
      "type": "string",
      "title": "Номер ОСБ",
      "maxLength": 2000
    },
    "vspNumber": {
      "type": "string",
      "title": "Номер ВСП",
      "maxLength": 2000
    },
    "imsi": {
      "type": "string",
      "title": "Mеждународный идентификатор мобильного абонента",
      "maxLength": 255
    }
  },
  "required": [
    "digitalId"
  ]
}
