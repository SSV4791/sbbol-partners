{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "javaType": "ru.sberbank.pprb.sbbol.partners.model.Document",
  "type": "object",
  "title": "Документ",
  "properties": {
    "id": {
      "type": "string",
      "title": "Идентификатор"
    },
    "digitalId": {
      "type": "string",
      "title": "Идентификатор личного кабинета"
    },
    "unifiedId": {
      "type": "string",
      "title": "Идентификатор партнера или контакта"
    },
    "version": {
      "type": "integer",
      "format": "int64",
      "title": "Версия"
    },
    "documentType": {
      "$ref": "document_type.json",
      "title": "Тип документа"
    },
    "series": {
      "type": "string",
      "title": "Серия документа"
    },
    "number": {
      "type": "string",
      "title": "Номер документа"
    },
    "dateIssue": {
      "type": "string",
      "format": "date",
      "title": "Дата выдачи документа"
    },
    "divisionIssue": {
      "type": "string",
      "title": "Место выдачи документа"
    },
    "divisionCode": {
      "type": "string",
      "title": "Код документа"
    },
    "certifierName": {
      "type": "string",
      "title": "Имя удостоверяющего сотрудника"
    },
    "positionCertifier": {
      "type": "string",
      "title": "Должность удостоверяющего сотрудника"
    },
    "certifierType": {
      "type": "string",
      "title": "Тип удостоверяющего сотрудника",
      "enum": [
        "NOTARY",
        "UNKNOWN"
      ]
    }
  }
}

