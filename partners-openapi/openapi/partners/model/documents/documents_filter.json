{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "title": "Фильтр для поиска документов",
  "properties": {
    "unifiedIds": {
      "type": "array",
      "x-extra-field-annotation": "@UuidFormatArrayValidation",
      "maximum": 30,
      "items": {
        "type": "string",
        "minLength": 36,
        "maxLength": 36,
        "x-not-empty": true
      },
      "title": "Идентификаторы партнера/контакта"
    },
    "documentType": {
      "type": "string",
      "title": "Тип документа"
    },
    "digitalId": {
      "type": "string",
      "title": "Идентификатор личного кабинета",
      "minLength": 1,
      "maxLength": 40,
      "x-not-empty": true
    },
    "pagination": {
      "$ref": "../common/pagination.json",
      "title": "Параметры запроса Документов"
    }
  },
  "required": [
    "unifiedIds",
    "digitalId",
    "pagination"
  ]
}
